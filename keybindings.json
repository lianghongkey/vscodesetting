        "when": "terminalFocus"
    },
    {
        "key": "ctrl+shift+e",
        "command": "workbench.files.action.focusOpenEditorsView",
        "when": "workbench.explorer.openEditorsView.active"
    },
    {
        "key": "ctrl+shift+g",
        "command": "scm.mainPanel.focus"
    },
    {
        "key": "ctrl+shift+o",
        "command": "sshHosts.focus"
    }
]
